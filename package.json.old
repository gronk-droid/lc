        "uiTheme": "vs-dark",
        "path": "./themes/lc chroma-color-theme.json"
      },
      {
        "label": "lc soft",
        "uiTheme": "vs-dark",
        "path": "./themes/lc soft-color-theme.json"
      },
      {
        "label": "lc soft chroma",
        "uiTheme": "vs-dark",
        "path": "./themes/lc soft chroma-color-theme.json"
      },
      {
        "label": "lc light",
        "uiTheme": "vs",
        "path": "./themes/lc light-color-theme.json"
      },
      {
        "label": "lc light chroma",
        "uiTheme": "vs",
        "path": "./themes/lc light chroma-color-theme.json"
      },
      {
        "label": "lc light soft",
        "uiTheme": "vs",
        "path": "./themes/lc light soft-color-theme.json"
      },
      {
        "label": "lc light soft chroma",
