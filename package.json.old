{
  "name": "Low Contrast",
  "displayName": "Low Contrast",
  "description": "A dark, low contrast theme based on [sema](https://github.com/lunacookies/sema).",
  "publisher": "gronk-droid",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/gronk-droid/lowcontrast"
  },
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "package": "vsce package"
  },
  "contributes": {
    "themes": [
      {
        "label": "lc",
        "uiTheme": "vs-dark",
